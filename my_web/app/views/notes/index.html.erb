<p id="notice"><%= notice %></p>

<h1>Notes</h1>

<table>
  <thead>
    <tr>
      <th>Genre</th>
      <th>Title</th>
      <th>Detail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <div class="genre">
      Genre 1
    </div>
    <div class="rectangle">
      Rectangle
    </div>
    <div class="desktop">
      <div class="texts">
	<div class="texts_frame">
	  <% @notes.each do |note| %>
	  <tr>
	    <td><%= note.genre %></td>
	    <td><%= note.title %></td>
	    <td><%= note.detail %></td>
	    <td><%= link_to 'Show', note %></td>
	    <td><%= link_to 'Edit', edit_note_path(note) %></td>
	    <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	  </tr>
	  <% end %>
	</div>
      </div>
      <div class="genres">
	<div class="genre">
	  <div class="genre_frame">
	  </div>
	</div>
	<div class="genres_frame">
	</div>
      </div>
      <div class="title">
	<div class="title_frame">
	</div>
	<div class="title_string">
	</div>
      </div>
    </div>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_note_path %>
